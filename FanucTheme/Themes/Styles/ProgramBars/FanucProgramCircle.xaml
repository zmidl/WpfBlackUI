<ResourceDictionary 
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:program="clr-namespace:FanucTheme.Controls.ProgramBars"
  xmlns:o="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
  <Style o:Freeze="True" TargetType="program:ProgramCircle">
    <Setter Property="DotDiameter" Value="4"/>
    <Setter Property="DotInterval" Value="10"/>
    <Setter Property="DotOffSet" Value="90"/>
    <Setter Property="DotSpeed" Value="4"/>
    <Setter Property="DotDelayTime" Value="110"/>
    <Setter Property="Width" Value="33"/>
    <Setter Property="Height" Value="33"/>
    <Setter Property="Effect" Value="{x:Null}"/>
    <Setter Property="Foreground" Value="{DynamicResource ControlForegroundBrush }"/>
  </Style>
</ResourceDictionary>